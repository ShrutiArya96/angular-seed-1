  "scripts": {
    "reinstall": "rimraf node_modules && npm cache clean && npm install",
    "build": "gulp build --env dev",
    "build:production": "gulp build --env production",
    "serve": "node build/serve.js --env dev",
    "prestart": "npm run build -s",
    "start": "parallelshell \"npm run serve -s\" \"npm run watch -s\"",
    "dev": "npm start",
    "lint": "eslint . --fix",
    "pretest": "npm run lint -s",
    "test": "gulp test",
    "watch": "gulp watch",
    "postversion": "git push && git push --tags",
    "predeploy": "npm run build:production",
    "deploy": "firebase deploy --project PROJECT_NAME"
  },
